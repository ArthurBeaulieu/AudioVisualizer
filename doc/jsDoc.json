{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["./src/js"],
    "exclude": ["./dist/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "name": "AudioVisualizer",
    "footerText": "AudioVisualizer",
    "cleverLinks": false,
    "monospaceLinks": false,
    "logo": {
      "url": "../demo/logo.png",
      "width": "20px",
      "height": "20px",
      "link": "https://github.com/ArthurBeaulieu/AudioVisualizer"
    }
  },
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "destination": "./doc/",
    "readme": "./README.md",
    "package": "./package.json",
    "access": "all",
    "recurse": true,
    "verbose": true,
    "private": true
  }
}
