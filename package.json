{
  "name": "audiovisualizer",
  "version": "0.9.4",
  "description": "Web module to display audio visualization using WebAudioAPI.",
  "keywords": [
    "javascript",
    "es6-module",
    "oscilloscope",
    "peakmeter",
    "bars",
    "spectrum",
    "waveform",
    "timeline"
  ],
  "main": "src/js/AudioVisualizer.js",
  "scripts": {
    "build": "webpack --env.version=0.9.4 --progress",
    "dev": "webpack --env.version=0.9.4 --env.dev=true --progress",
    "web-server": "http-server -p 1337 -c-1 -o example.html",
    "doc": "jsdoc -c doc/jsDoc.json",
    "beforecommit": "npm run build && npm run doc"
  },
  "homepage": "https://github.com/ArthurBeaulieu/AudioVisualizer/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArthurBeaulieu/AudioVisualizer.git"
  },
  "bugs": {
    "url": "https://github.com/ArthurBeaulieu/AudioVisualizer/issues"
  },
  "author": "Arthur Beaulieu",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": ">=7.1.2",
    "@babel/preset-env": ">=7.11.0",
    "babel-loader": ">=8.0.4",
    "clean-webpack-plugin": ">=3.0.0",
    "css-loader": ">=1.0.0",
    "http-server": ">=0.12.3",
    "jasmine": ">=3.2.0",
    "jasmine-core": ">=3.3.0",
    "jsdoc": ">=3.5.5",
    "karma": ">=5.0.9",
    "karma-chrome-launcher": ">=2.2.0",
    "karma-firefox-launcher": ">=1.1.0",
    "karma-jasmine": ">=1.1.2",
    "karma-sourcemap-loader": ">=0.3.7",
    "karma-webpack": ">=4.0.0",
    "mini-css-extract-plugin": ">=0.4.5",
    "node-sass": ">=4.13.1",
    "optimize-css-assets-webpack-plugin": ">=5.0.1",
    "sass-loader": ">=7.1.0",
    "tui-jsdoc-template": ">=1.2.2",
    "webpack": ">=4.16.5",
    "webpack-cli": ">=3.1.0"
  }
