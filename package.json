{
  "name": "audiovisualizer",
  "version": "0.9.7",
  "description": "Web module to display audio visualization using WebAudioAPI.",
  "keywords": [
    "javascript",
    "es6-module",
    "oscilloscope",
    "peakmeter",
    "bars",
    "spectrum",
    "waveform",
    "timeline"
  ],
  "main": "src/js/AudioVisualizer.js",
  "scripts": {
    "build": "webpack --config ./src/tools/webpack.prod.js ",
    "watch": "webpack --watch --config ./src/tools/webpack.dev.js",
    "server": "ws --spa index.html --port 1337 --http2",
    "doc": "jsdoc -c doc/jsDoc.json",
    "beforecommit": "npm run build && npm run doc"
  },
  "homepage": "https://github.com/ArthurBeaulieu/AudioVisualizer/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArthurBeaulieu/AudioVisualizer.git"
  },
  "bugs": {
    "url": "https://github.com/ArthurBeaulieu/AudioVisualizer/issues"
  },
  "author": "Arthur Beaulieu",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.13.0",
    "eslint-webpack-plugin": "^2.2.1",
    "file-loader": "^6.2.0",
    "jsdoc": "^3.6.6",
    "local-web-server": "^4.2.1",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^4.14.1",
    "postcss": "^8.2.10",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "tui-jsdoc-template": "^1.2.2",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.3.0"
  }
}
